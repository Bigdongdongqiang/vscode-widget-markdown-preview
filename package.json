{
  "name": "vscode-widget-markdown-preview",
  "displayName": "Markdown 实时预览",
  "description": "左侧编辑、右侧实时预览 Markdown。",
  "version": "0.1.0",
  "publisher": "bigdongdongqiang",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdownLivePreview.open",
        "title": "Markdown: 打开实时预览（右侧）"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "markdownLivePreview.open",
          "when": "editorLangId == markdown",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run copy-assets",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "copy-assets": "node scripts/copy-marked.js"
  },
  "dependencies": {
    "marked": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bigdongdongqiang/vscode-widget-markdown-preview.git"
  },
  "license": "MIT",
  "keywords": [
    "markdown",
    "preview",
    "实时预览"
  ]
}
